{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "semistandard",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": [
            0
        ],
        "quote-props": [
            "error",
            "as-needed",
            {
                "keywords": true,
                "unnecessary": false
            }
        ],
        "indent": [
            1,
            2
        ],
        "space-before-function-paren": [
            0
        ]
    }
}